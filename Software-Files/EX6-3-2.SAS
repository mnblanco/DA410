/* EXAMPLE 6.3.2  */

TITLE 'EXAMPLE 6.3.2 ';
DATA ROOT;
  INFILE 'T6_2_ROOT.dat';
  INPUT STOCK GIRTH4 GROWTH GIRTH15 WEIGHT;
PROC GLM;
  CLASS STOCK;
  MODEL GIRTH4 GROWTH GIRTH15 WEIGHT = STOCK;
  CONTRAST 'ENDS vs. MIDDLE'
    STOCK 2 -1 -1 -1 -1 2;
  CONTRAST 'FIRST vs. LAST'
    STOCK 1 0 0 0 0 -1;
  MANOVA H=STOCK/PRINTE PRINTH;
RUN;