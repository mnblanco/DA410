/*  EXAMPLES 11.3 and 11.4.1  */

FILENAME CHEM 'T10_1_CHEM.dat';
TITLE 'EXAMPLES 11.3 and 11.4.1';
DATA CHEM;
  INFILE CHEM;
  INPUT NUM Y1 Y2 Y3 X1 X2 X3;
  X1X2 = X1 * X2;     X1SQ = X1 * X1;
  X1X3 = X1 * X3;     X2SQ = X2 * X2;
  X2X3 = X2 * X3;     X3SQ = X3 * X3;
PROC CANCORR ALL
  VPREFIX = INPUT VNAME = 'INPUT VARIABLES'
  WPREFIX = YIELD WNAME = 'YIELD VARIABLES';
  WITH Y1 Y2 Y3;
  VAR X1 X2 X3 X1X2 X1X3 X2X3 X1SQ X2SQ X3SQ;
ENDSAS;